#include <stdio.h>
#include <string.h>

int main() {
    int n, i, low, high, mid;
    char names[100][100], target[100];

    printf("Enter number of names: ");
    scanf("%d", &n);

    printf("Enter %d names (sorted alphabetically):\n", n);
    for (i = 0; i < n; i++) scanf("%s", names[i]);

    printf("Enter name to search: ");
    scanf("%s", target);

    low = 0; high = n - 1;
    while (low <= high) {
        mid = (low + high) / 2;
        int cmp = strcmp(names[mid], target);
        if (cmp == 0) { printf("Found at position %d\n", mid + 1); return 0; }
        if (cmp < 0) low = mid + 1;
        else high = mid - 1;
    }
    printf("Not found\n");
    return 0;
}
