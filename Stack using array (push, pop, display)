#include <stdio.h>

int stack[100], top = -1;

void push(int x) { if (top < 99) stack[++top] = x; }
void pop() { if (top >= 0) top--; }
void display() { for (int i = 0; i <= top; i++) printf("%d ", stack[i]); printf("\n"); }

int main() {
    push(10); push(20); push(30);
    display();
    pop();
    display();
    return 0;
}
